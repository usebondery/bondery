{
  "name": "@bondery/branding",
  "version": "0.1.0",
  "type": "module",
  "description": "Bondery branding assets and icons",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./icon-generator": {
      "import": "./scripts/generate-icons.ts",
      "require": "./scripts/generate-icons.ts",
      "types": "./scripts/icon-generator.d.ts"
    },
    "./icons": {
      "import": "./src/react/index.ts",
      "require": "./src/react/index.ts",
      "types": "./src/react/index.d.ts"
    },
    "./react": {
      "types": "./src/react/index.d.ts",
      "import": "./src/react/index.ts",
      "require": "./src/react/index.ts"
    },
    "./theme": {
      "import": "./src/theme.ts",
      "require": "./src/theme.ts"
    },
    "./styles": {
      "style": "./src/styles.css",
      "import": "./src/styles.css",
      "require": "./src/styles.css"
    },
    "./src/styles.css": {
      "style": "./src/styles.css",
      "import": "./src/styles.css",
      "require": "./src/styles.css"
    }
  },
  "files": [
    "src",
    "scripts"
  ],
  "scripts": {
    "build": "npm run generate-react-icons",
    "generate-react-icons": "tsx scripts/generate-react-icons.ts"
  },
  "dependencies": {
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@types/node": "^20",
    "tsx": "^4.19.2",
    "typescript": "^5"
  },
  "peerDependencies": {
    "@mantine/core": "^8.3.11"
  },
  "keywords": [
    "branding",
    "icons",
    "assets"
  ]
}
