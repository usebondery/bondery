{
  "name": "@bondery/branding",
  "version": "0.1.0",
  "type": "module",
  "description": "Bondery branding assets and icons",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./src": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./icon-generator": {
      "import": "./scripts/generate-icons.ts",
      "require": "./scripts/generate-icons.ts",
      "types": "./scripts/icon-generator.d.ts"
    },
    "./icons": {
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.js"
    },
    "./react/src": {
      "types": "./src/react/index.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.js"
    }
  },
  "files": [
    "dist",
    "scripts",
    "src/assets"
  ],
  "scripts": {
    "build": "tsc",
    "generate-react-icons": "tsx scripts/generate-react-icons.ts"
  },
  "dependencies": {
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@types/node": "^25",
    "tsx": "^4.21.0",
    "typescript": "^5"
  },
  "keywords": [
    "branding",
    "icons",
    "assets"
  ]
}
