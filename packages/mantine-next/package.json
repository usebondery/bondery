{
  "name": "@bondery/mantine-next",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./theme": {
      "import": "./src/theme.ts",
      "require": "./src/theme.ts",
      "types": "./src/theme.ts"
    },
    "./theme/src": {
      "import": "./src/theme.ts",
      "require": "./src/theme.ts",
      "types": "./src/theme.ts"
    },
    "./styles": {
      "style": "./src/styles.css",
      "import": "./src/styles.css",
      "require": "./src/styles.css"
    }
  },
  "dependencies": {
    "@bondery/branding": "*"
  },
  "peerDependencies": {
    "@mantine/core": "^8.3.11",
    "next": "^16.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
